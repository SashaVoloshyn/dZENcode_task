{
    "name": "backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "rimraf dist && tsc-watch --onSuccess \"ts-node ./src/app.ts\"",
        "lint": "npx eslint --cache --ext .ts src/",
        "lint:fix": "npm run lint -- --fix",
        "prettier:write": "npx prettier --write",
        "format": "npm run prettier:write src",
        "format:all": "npm run prettier:write .",
        "format:conflict": "npx eslint-config-prettier src/**",
        "typeorm": "typeorm-ts-node-commonjs",
        "migration:create": "typeorm migration:create",
        "migration:generate": "npm run typeorm -- -d ./src/configs/ormconfig.ts migration:generate",
        "migration:run": "npm run typeorm migration:run -- -d ./src/configs/ormconfig.ts",
        "migration:revert": "npm run typeorm migration:revert -- -d ./src/configs/ormconfig.ts"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.1323.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.2",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "helmet": "^6.0.1",
        "joi": "^17.8.3",
        "jsonwebtoken": "^9.0.0",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.4.0",
        "redis": "^4.6.4",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.6",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/aws-sdk": "^2.7.0",
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/joi": "^17.2.3",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.14.0",
        "@types/prettier": "^2.7.2",
        "@types/reflect-metadata": "^0.1.0",
        "@types/tsc-watch": "^4.2.0",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "eslint": "^8.22.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "2.27.5",
        "nodemon": "^2.0.18",
        "prettier": "2.8.4",
        "rimraf": "^4.1.2",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.0",
        "typescript": "^4.9.5"
    }
}
